{
  "name": "@saleor/configurator",
  "version": "1.1.0",
  "type": "module",
  "packageManager": "pnpm@10.28.1",
  "main": "dist/main.js",
  "bin": {
    "@saleor/configurator": "./bin/cli.mjs",
    "configurator": "./bin/cli.mjs"
  },
  "files": [
    "dist",
    "bin",
    "recipes",
    "README.md",
    "LICENSE",
    "schema.json",
    "SCHEMA.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "saleor": {
    "schemaVersion": "3.20"
  },
  "scripts": {
    "dev": "tsx src/cli/main.ts",
    "deploy": "tsx src/cli/main.ts deploy",
    "introspect": "tsx src/cli/main.ts introspect",
    "diff": "tsx src/cli/main.ts diff",
    "start": "tsx src/cli/main.ts start",
    "fetch-schema": "tsx src/lib/graphql/fetch-schema.ts",
    "copy-recipes": "cp -r src/recipes dist/recipes",
    "build": "tsup src/cli/main.ts --format esm --target node20 --clean --shims && pnpm copy-recipes",
    "prepublishOnly": "pnpm build",
    "generate-json-schema": "tsx scripts/generate-json-schema.ts",
    "generate-schema-docs": "tsx scripts/generate-schema-docs.ts",
    "prepare": "husky",
    "test": "vitest",
    "test:ci": "CI=true vitest --silent",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset",
    "version": "changeset version",
    "publish:ci-prod": "pnpm publish --no-git-checks && pnpm exec changeset tag && git push --follow-tags",
    "publish:ci-dev": "pnpm exec changeset version --snapshot pr && pnpm publish --tag dev --no-git-checks",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "check:ci": "biome lint . --diagnostic-level=error"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.5",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.4",
    "@types/node": "^20.19.8",
    "@types/opossum": "^8.1.9",
    "execa": "^8.0.1",
    "husky": "^9.1.7",
    "msw": "^2.4.9",
    "nock": "^13.5.0",
    "strip-ansi": "^7.1.0",
    "tmp": "^0.2.1",
    "tree-kill": "^1.2.2",
    "tsup": "^8.5.0",
    "tsx": "^4.7.0",
    "typescript": "^5.5.4",
    "vitest": "^3.0.4"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@inquirer/prompts": "^7.6.0",
    "@urql/core": "^5.0.6",
    "@urql/exchange-auth": "^2.2.0",
    "@urql/exchange-retry": "^2.0.0",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "dotenv": "^16.4.5",
    "gql.tada": "^1.8.5",
    "opossum": "^9.0.0",
    "ora": "^8.2.0",
    "p-limit": "^7.3.0",
    "p-retry": "^7.1.1",
    "tiny-invariant": "^1.3.3",
    "tslog": "^4.9.3",
    "xlsx": "^0.18.5",
    "yaml": "^2.8.0",
    "zod": "^4.0.0"
  }
}
